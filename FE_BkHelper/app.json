{
  "expo": {
    "name": "BkHelper",
    "slug": "BkHelper",
    "owner": "thien.caolehoan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon3.png",
    "scheme": "bkhelper",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/customize-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0794DE"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "y",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.BkHelper",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "sentry-expo",
        {
          "organization": "bach-khoa-university-pi",
          "project": "react-native"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5f1bf7e1-2f59-4ded-8421-032aa205a56e"
      }
      // "sentryDsn": "https://48cd84ef14e78358fd23d6a172b8da29@o4510499653419008.ingest.us.sentry.io/4510502713491456"
    },
    "updates": {
      "url": "https://u.expo.dev/5f1bf7e1-2f59-4ded-8421-032aa205a56e"
    }
  }
}
